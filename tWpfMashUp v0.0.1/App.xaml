                <ResourceDictionary Source="/Assets/Templates/MessegeTextBox.xaml" />
                <ResourceDictionary Source="/Assets/Templates/ContectListItem.xaml" />
                <ResourceDictionary Source="/Assets/Templates/MessageListItem.xaml" />
                <ResourceDictionary Source="/Assets/Templates/ScrollBar.xaml"/>
                <ResourceDictionary Source="/Assets/Templates/TurnIndicatorRadioBotton.xaml"/>
                <ResourceDictionary Source="/Assets/Templates/CustomCombobox.xaml"/>
                <ResourceDictionary Source="/Assets/Templates/ToggleButtonTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
